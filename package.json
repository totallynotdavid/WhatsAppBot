{
  "name": "whatsapp-bot",
  "version": "1.9.5",
  "description": "Un bot de WhatsApp multifuncional.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
		"start:prod": "pm2 start index.js --cron-restart=\"*/12 * * * *\" && npm run logs",
		"logs": "nano ~/.pm2/logs/index-out.log",
    "clean": "npm run clean:mundane && npm run clean:logs && npm run clean:packages && npm run clean:session && npm i && npm run start:prod",
    "clean:logs": "shx rm ~/.pm2/logs/index-out.log ~/.pm2/logs/index-error.log",
    "clean:mundane": "shx rm -rf music logs.txt",
    "clean:packages": "shx rm -rf node_modules package-lock.json",
    "clean:session": "shx rm -rf .wwebjs_auth",
    "lint": "eslint commands functions lib index.js",
    "lint:fix": "eslint commands functions lib index.js --fix",
		"postinstall": "npm i shx -g"
  },
  "author": "totallynotdavid",
  "license": "MIT",
  "devDependencies": {
    "@google-cloud/local-auth": "^2.1.0",
    "@supabase/supabase-js": "^2.7.1",
    "aws-sdk": "^2.1386.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.31.0",
    "facebook-nodejs-business-sdk": "^16.0.2",
    "googleapis": "^105.0.0",
    "moment": "^2.29.4",
    "node-fetch": "^2.6.7",
    "openai": "^3.2.1",
    "qrcode-terminal": "^0.12.0",
    "shx": "^0.3.4",
    "spotify-web-api-node": "^5.0.2",
    "sqlite3": "^5.1.6",
    "whatsapp-web.js": "^1.21.0"
  }
}
