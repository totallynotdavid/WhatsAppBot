{
  "name": "whatsapp-bot",
  "version": "1.9.7",
  "description": "Un bot de WhatsApp multifuncional",
  "author": "totallynotdavid",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "lint": "eslint **/*.js",
    "prettier": "prettier --write .",
    "start": "node index.js",
    "start:prod": "pnpm run clean:logs && pm2 start index.js --cron-restart=\"*/45 * * * *\" && pnpm run logs",
    "test": "pnpm run -s lint",
    "clean": "pnpm run clean:logs && pnpm run clean:mundane && pnpm run clean:packages && pnpm run clean:session && pnpm i",
    "clean:logs": "shx rm -rf ~/.pm2/logs",
    "clean:mundane": "shx rm -rf music logs.txt",
    "clean:packages": "shx rm -rf node_modules",
    "clean:session": "shx rm -rf .wwebjs_auth",
    "logs": "pm2 log",
    "postinstall": "pnpm i pm2 -g"
  },
  "dependencies": {
    "@aws-sdk/client-polly": "^3.370.0",
    "@google-cloud/local-auth": "^2.1.0",
    "@supabase/supabase-js": "^2.7.1",
    "@vitalets/google-translate-api": "^9.2.0",
    "bimg": "^0.1.5",
    "discord-image-generation": "^1.4.25",
    "dotenv": "^16.0.3",
    "facebook-nodejs-business-sdk": "^18.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "googleapis": "^105.0.0",
    "imgur": "^2.3.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "openai": "^3.2.1",
    "qrcode-terminal": "^0.12.0",
    "spotify-web-api-node": "^5.0.2",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "whatsapp-web.js": "github:pedroslopez/whatsapp-web.js#72a18816a921bf0cc762cd805cc1141d22711d3c",
    "yt_duration": "^0.0.2",
    "yt_metadata": "^0.0.5",
    "ytdlp_video_processor": "^0.0.6"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^1.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "3.1.1",
    "shx": "^0.3.4"
  }
}