{
  "name": "whatsapp-bot",
  "version": "1.9.7",
  "description": "Un bot de WhatsApp multifuncional.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:prod": "pnpm run clean:logs && pm2 start index.js --cron-restart=\"*/45 * * * *\" && pnpm run logs",
    "logs": "nano ~/.pm2/logs/index-out.log",
    "clean": "pnpm run clean:mundane && pnpm run clean:logs && pnpm run clean:packages && pnpm run clean:session && pnpm i && pnpm run start:prod",
    "clean:logs": "shx rm ~/.pm2/logs/index-out.log ~/.pm2/logs/index-error.log",
    "clean:mundane": "shx rm -rf music logs.txt",
    "clean:packages": "shx rm -rf node_modules",
    "clean:session": "shx rm -rf .wwebjs_auth",
    "lint": "eslint commands functions lib index.js",
    "lint:fix": "eslint --fix commands functions lib index.js",
    "test": "pnpm run -s lint"
  },
  "author": "totallynotdavid",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-polly": "^3.370.0",
    "@google-cloud/local-auth": "^2.1.0",
    "@supabase/supabase-js": "^2.7.1",
    "@vitalets/google-translate-api": "^9.2.0",
    "bimg": "^0.1.5",
    "discord-image-generation": "^1.4.25",
    "dotenv": "^16.0.3",
    "facebook-nodejs-business-sdk": "^18.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "googleapis": "^105.0.0",
    "imgur": "^2.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-fetch": "^2.6.7",
    "openai": "^3.2.1",
    "qrcode-terminal": "^0.12.0",
    "spotify-web-api-node": "^5.0.2",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "whatsapp-web.js": "github:pedroslopez/whatsapp-web.js#b5eb4f97ce0c7f9a63275841bef4e61d0e4b4db4",
    "yt_metadata": "^0.0.4"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "3.1.1",
    "shx": "^0.3.4"
  }
}
